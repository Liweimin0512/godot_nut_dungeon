config_version=5

[application]

config/name="godot-turn-based-gameplay-framework"
run/main_scene="res://scenes/core/main.tscn"
config/features=PackedStringArray("4.4", "Mobile")
config/icon="res://icon.svg"

[autoload]

CombatSystem="*res://scripts/systems/combat/combat_system.gd"
UIManager="*res://addons/GodotUIFramework/source/ui_manager.gd"
EffectNodeFactory="*res://addons/godot_gameplay_ability_system/source/common/factory/ability_effect_node_factory.gd"
EventBus="*res://addons/li_framework/event_bus.gd"
Console="*res://addons/console/console.gd"
Data="*res://addons/godot_tfdp/Data.gd"
SignalLens="*res://addons/signal_lens/autoload/signal_lens_autoload.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"
DataManager="*res://addons/GDDataForge/source/data_manager.gd"

[dialogic]

directories/dch_directory={}
directories/dtl_directory={}

[display]

window/size/viewport_width=576
window/size/viewport_height=324
window/size/window_width_override=1152
window/size/window_height_override=648
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/GDDataForge/plugin.cfg", "res://addons/GodotUIFramework/plugin.cfg", "res://addons/godot_gameplay_ability_system/plugin.cfg", "res://addons/gut/plugin.cfg")

